use("sample_training")

var documentoExistente = db.zips.findOne({ _id: 1 });

if (documentoExistente) {
    // Elimina el documento existente
    db.zips.deleteOne({ _id: 1 });

    // Inserta el documento con nuevo _id
    documentoExistente._id = null;
    db.zips.insertOne(documentoExistente);
}

// Muestra los documentos actualizados
db.zips.find();